{
  "blocks": {

  },
  "directories": {
    "srcPath": "./src/",
    "buildPath": "./build/",
    "blocksDirName": "blocks",
    "public": {
      "css": "./build/css",
      "images": "./build/img",
      "js": "./build/js",
      "fonts": "./build/fonts"
    },
    "src": {
      "html": "./src/*.html",
      "js": "./src/js/*.js",
      "images": "./src/img/**/*.*",
      "fonts": "./src/fonts/**/*.*"
    },
    "watch": {
      "html": "./src/**/*.html",
      "style": "./src/**/*.scss",
      "js": "./src/js/**/*.js",
      "images": "./src/img/**/*.*",
      "fonts": "./src/fonts/**/*.*"
    }
  },
  "addCssBefore": [
    "./src/scss/global/variables.scss",
    "./src/scss/global/global.scss",
    "./src/scss/global/fonts.scss",
    "./src/scss/global/mixins.scss"
  ],
  "addCssAfter": [],
  "addJsBefore": [],
  "addJsAfter": []
}
